{
  "name": "apollo-mui-ssr-example",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Maximilian Fellner <max.fellner@gmail.com.com>",
  "description": "Example of Apollo GraphQL, Material UI and SSR.",
  "scripts": {
    "webpack:development": "webpack --mode development",
    "webpack:production": "webpack --mode production",
    "webpack:watch": "yarn webpack:development --watch",
    "nodemon": "nodemon src/server.ts",
    "dev": "concurrently \"yarn webpack:watch\" \"yarn nodemon\"",
    "clean": "rimraf static/bundle/ .cache/ dist/",
    "build:server": "tsc --skipLibCheck --outDir dist/",
    "build": "yarn clean && concurrently \"yarn webpack:production\" \"yarn build:server\"",
    "start": "NODE_ENV=production node dist/server.js"
  },
  "dependencies": {
    "@material-ui/core": "1.4.0",
    "@material-ui/icons": "1.1.0",
    "apollo-cache-inmemory": "1.2.5",
    "apollo-client": "2.3.5",
    "apollo-link-http": "1.5.4",
    "apollo-link-schema": "1.1.0",
    "apollo-server-micro": "2.0.0",
    "graphql": "0.13",
    "graphql-tag": "2.9.2",
    "graphql-tools": "3.0.5",
    "micro": "9.3.2",
    "microrouter": "3.1.3",
    "react": "16.4.1",
    "react-apollo": "2.1.9",
    "react-dom": "16.4.1",
    "serve-handler": "3.3.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/graphql": "0.13.3",
    "@types/jss": "9.5.3",
    "@types/micro": "7.3.1",
    "@types/microrouter": "3.1.0",
    "@types/node": "10.5.2",
    "@types/prop-types": "15.5.3",
    "@types/react": "16.4.6",
    "@types/react-dom": "16.0.6",
    "@types/uuid": "3.4.3",
    "@types/webpack": "4.4.7",
    "concurrently": "3.6.0",
    "nodemon": "1.18.3",
    "rimraf": "2.6.2",
    "ts-loader": "4.4.2",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "2.9.2",
    "webpack": "4.16.1",
    "webpack-cli": "3.1.0"
  }
}
